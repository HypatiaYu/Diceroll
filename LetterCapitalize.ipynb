    "def LetterCapitalize(arg): \n",
    "    arg=arg.split(' ')\n",
    "    new = []\n",
    "    for i in arg: \n",
    "        new.append(i.capitalize())\n",
    "    \n",
    "    #convert list into a string \n",
    "    arg2 =' '.join(new)\n",
    "    return(arg2)"
   ]
